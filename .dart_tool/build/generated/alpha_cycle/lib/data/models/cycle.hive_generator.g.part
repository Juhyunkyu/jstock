class CycleAdapter extends TypeAdapter<Cycle> {
  @override
  final int typeId = 1;

  @override
  Cycle read(BinaryReader reader) {
    final numOfFields = reader.readByte();
    final fields = <int, dynamic>{
      for (int i = 0; i < numOfFields; i++) reader.readByte(): reader.read(),
    };
    return Cycle(
      id: fields[0] as String,
      ticker: fields[1] as String,
      cycleNumber: fields[2] as int,
      seedAmount: fields[3] as double,
      initialEntryPrice: fields[5] as double,
      exchangeRate: fields[17] as double,
      buyTrigger: fields[12] as double,
      sellTrigger: fields[13] as double,
      panicTrigger: fields[14] as double,
      startDate: fields[15] as DateTime?,
    )
      ..averagePrice = fields[6] as double
      ..totalShares = fields[7] as double
      ..totalInvestedAmount = fields[8] as double
      ..remainingCash = fields[9] as double
      ..panicUsed = fields[10] as bool
      ..status = fields[11] as CycleStatus
      ..endDate = fields[16] as DateTime?;
  }

  @override
  void write(BinaryWriter writer, Cycle obj) {
    writer
      ..writeByte(18)
      ..writeByte(0)
      ..write(obj.id)
      ..writeByte(1)
      ..write(obj.ticker)
      ..writeByte(2)
      ..write(obj.cycleNumber)
      ..writeByte(3)
      ..write(obj.seedAmount)
      ..writeByte(4)
      ..write(obj.initialEntryAmount)
      ..writeByte(5)
      ..write(obj.initialEntryPrice)
      ..writeByte(6)
      ..write(obj.averagePrice)
      ..writeByte(7)
      ..write(obj.totalShares)
      ..writeByte(8)
      ..write(obj.totalInvestedAmount)
      ..writeByte(9)
      ..write(obj.remainingCash)
      ..writeByte(10)
      ..write(obj.panicUsed)
      ..writeByte(11)
      ..write(obj.status)
      ..writeByte(12)
      ..write(obj.buyTrigger)
      ..writeByte(13)
      ..write(obj.sellTrigger)
      ..writeByte(14)
      ..write(obj.panicTrigger)
      ..writeByte(15)
      ..write(obj.startDate)
      ..writeByte(16)
      ..write(obj.endDate)
      ..writeByte(17)
      ..write(obj.exchangeRate);
  }

  @override
  int get hashCode => typeId.hashCode;

  @override
  bool operator ==(Object other) =>
      identical(this, other) ||
      other is CycleAdapter &&
          runtimeType == other.runtimeType &&
          typeId == other.typeId;
}

class CycleStatusAdapter extends TypeAdapter<CycleStatus> {
  @override
  final int typeId = 10;

  @override
  CycleStatus read(BinaryReader reader) {
    switch (reader.readByte()) {
      case 0:
        return CycleStatus.active;
      case 1:
        return CycleStatus.completed;
      case 2:
        return CycleStatus.cancelled;
      default:
        return CycleStatus.active;
    }
  }

  @override
  void write(BinaryWriter writer, CycleStatus obj) {
    switch (obj) {
      case CycleStatus.active:
        writer.writeByte(0);
        break;
      case CycleStatus.completed:
        writer.writeByte(1);
        break;
      case CycleStatus.cancelled:
        writer.writeByte(2);
        break;
    }
  }

  @override
  int get hashCode => typeId.hashCode;

  @override
  bool operator ==(Object other) =>
      identical(this, other) ||
      other is CycleStatusAdapter &&
          runtimeType == other.runtimeType &&
          typeId == other.typeId;
}
